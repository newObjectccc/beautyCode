{
  "name": "beautycode",
  "version": "1.0.0",
  "description": "A VS Code extension that exports beautiful code block images based on the user's selected code.",
  "publisher": "Vesper@之一",
  "main": "./out/extension.js",
  "scripts": {
    "vsc:publish": "npx @vscode/vsce publish",
    "vsc:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newObjectccc/beautyCode.git"
  },
  "keywords": [
    "beauty",
    "codeblock",
    "code",
    "export",
    "png",
    "picture"
  ],
  "displayName": "Beauty CodeBlock Exporter",
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "icon": "images/iconLg-48x48px.png",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "extension.beautyCode",
        "title": "beautyCode: generate beauty CodeBlock"
      }
    ],
    "keybindings": [
      {
        "command": "extension.beautyCode",
        "key": "ctrl+f5",
        "mac": "cmd+f5",
        "when": "editorTextFocus"
      }
    ]
  },
  "author": "Vesper@之一",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vscode-use/utils": "^0.0.86",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
