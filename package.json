{
  "name": "vscode-extension-boilerplate",
  "version": "1.0.0",
  "description": "",
  "publisher": "Vesper@之一",
  "main": "./out/extension.js",
  "scripts": {
    "publish": "npx @vscode/vsce publish",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepare": "husky"
  },
  "keywords": [],
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "icon": "images/icon.png",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.smartConsole",
        "title": "smart log"
      }
    ],
    "keybindings": [
      {
        "command": "extension.smartConsole",
        "key": "ctrl+f5",
        "mac": "cmd+f5",
        "when": "editorTextFocus"
      }
    ]
  },
  "author": "Vesper@之一",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vscode-use/utils": "^0.0.86",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
